@model Magazine.Models.Enterprise

@{
    ViewBag.Title = "Delete";
}

<h3>Действительно хотите удалить эту запись?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
           <h5><b>Форма собственности</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.PropForm)</h5>
        </dd>

        <dt>
            <h5><b>Название</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.Name)</h5>
        </dd>

        <dt>
            <h5><b>Фамилия</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.BossLastName)</h5>
        </dd>

        <dt>
            <h5><b>Имя</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.BossFirstName)</h5>
        </dd>

        <dt>
            <h5><b>Отчество</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.BossMiddleName)</h5>
        </dd>

        <dt>
            <h5><b>Адрес</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.Address)</h5>
        </dd>

        <dt>
            <h5><b>Телефон</b></h5>
        </dt>

        <dd>
            <h5>@Html.DisplayFor(model => model.PhoneNumber)</h5>
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Удалить" class="btn btn-default" /> 
            @Html.ActionLink("Назад", "Index")
        </div>
    }
</div>
